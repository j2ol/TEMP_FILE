ALTER TABLE `181_realestate`.`data_analysis` 
ADD COLUMN `weighted_manual` int(11) NULL COMMENT '权重值-手动增加' AFTER `weighted`;
ALTER TABLE `181_realestate`.`data_analysis` 
MODIFY COLUMN `weighted` int(11) NULL DEFAULT 0.0 COMMENT '权重值' AFTER `view_manual`;
CREATE TABLE `data_analysis_weighted_config` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `data_type` varchar(32) NOT NULL COMMENT '数据类型(SECOND_HOUSE-二手房)',
  `view_proportion` int(5) DEFAULT '0' COMMENT '浏览量-比重',
  `collect_proportion` int(11) DEFAULT '0' COMMENT '收藏量-比重',
  `subscription_proportion` int(11) DEFAULT '0' COMMENT '房价订阅量-比重',
  `share_proportion` int(11) DEFAULT '0' COMMENT '分享量-比重',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1064 DEFAULT CHARSET=utf8 COMMENT='数据分析';
